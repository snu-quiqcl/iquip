{
    "app": {
        "explorer": {
            "module": "iquip.apps.explorer",
            "cls": "ExplorerApp",
            "show": true,
            "pos": "left"
        },
        "result_explorer": {
            "module": "iquip.apps.result_explorer",
            "cls": "ResultExplorerApp",
            "show": true,
            "pos": "right"
        },
        "logger": {
            "module": "iquip.apps.logger",
            "cls": "LoggerApp",
            "show": true,
            "pos": "bottom",
            "args": {
                "logFilePath": ""
            }
        },
        "scheduler": {
            "module": "iquip.apps.scheduler",
            "cls": "SchedulerApp",
            "show": true,
            "pos": "bottom"
        },
        "monitor": {
            "module": "iquip.apps.monitor",
            "cls": "DeviceMonitorApp",
            "show": true,
            "pos": "right",
            "args": {
                "ttlInfo": {
                    "TTL_test_0": "ttl0",
                    "TTL_test_1": "ttl1"
                },
                "dacInfo": {
                    "DAC_test_0": {
                        "device": "zotino0",
                        "channel": 0
                    },
                    "DAC_test_1": {
                        "device": "zotino0",
                        "channel": 1
                    }
                },
                "ddsInfo": {
                    "DDS_test_0": {
                        "device": "urukul0",
                        "channel": 0,
                        "frequencyInfo": {
                            "unit": "MHz"
                        }
                    },
                    "DDS_test_1": {
                        "device": "urukul0",
                        "channel": 3,
                        "amplitudeInfo": {
                            "step": 0.1
                        }
                    }
                }
            }
        },
        "dataviewer": {
            "module": "iquip.apps.dataviewer",
            "cls": "DataViewerApp",
            "show": true,
            "pos": "bottom"
        }
    },
    "constant": {
        "proxy_ip": "127.0.0.1",
        "proxy_port": 8000,
        "icon_path": "resources/icon.jpg",
        "background_path": "resources/background.jpg"
    }
}
